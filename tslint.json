{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "webpack.config.js",
            "src/lib/GM/GM_Frontend.js",
            "src/lib/parse/convert2RegExp.ts",
            "node_modules/**",
            "dist/**",
            "src/gui/libs/**"
        ]
    },
    "jsRules": {},
    "rules": {
        "indent": [true, "spaces", 4],
        "no-console" : [false],
        "class-name": false,
        "variable-name" : [false],
        "max-line-length" : [false]
    },
    "rulesDirectory": []
}