{
  "name": "usi",
  "version": "0.5.18",
  "description": "usi, userscript injector for Mozilla Firefox",
  "scripts": {
    "dev": "webpack --watch --mode development --devtool inline-source-map",
    "browser": "web-ext run -s dist -p usi",
    "browser-fresh": "web-ext run -s dist",
    "prod": "webpack --watch --mode production",
    "self-update-packages": "ncu -u --removeRange && npm install",
    "lint-fix": "tslint --fix -p .",
    "remove-dist-folder": "node scripts/remove-dist-folder.js",
    "build": "webpack --mode production && web-ext build -s dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/usi-firefox/usi.git"
  },
  "author": {
    "name": "usi"
  },
  "license": "LGPL-2.1-only",
  "dependencies": {
    "highlight.js": "10.4.0",
    "material-design-icons-iconfont": "6.1.0",
    "vue": "2.6.12",
    "vuetify": "2.3.18",
    "vuex": "3.5.1"
  },
  "devDependencies": {
    "@mdi/font": "5.8.55",
    "@mdi/js": "5.8.55",
    "@types/jquery": "3.5.4",
    "copy-webpack-plugin": "6.3.2",
    "css-loader": "5.0.1",
    "file-loader": "6.2.0",
    "npm-check-updates": "10.2.2",
    "remove-files-webpack-plugin": "1.4.4",
    "replace-in-file-webpack-plugin": "1.0.6",
    "style-loader": "2.0.0",
    "ts-loader": "8.0.11",
    "tslint": "6.1.3",
    "typescript": "4.1.2",
    "vue-loader": "15.9.5",
    "vue-template-compiler": "2.6.12",
    "web-ext": "5.4.0",
    "web-ext-types": "3.2.1",
    "webpack": "5.6.0",
    "webpack-cli": "4.2.0"
  }
}
