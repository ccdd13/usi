{
  "name": "usi",
  "version": "0.5.12",
  "description": "usi, userscript injector for Mozilla Firefox",
  "scripts": {
    "dev": "webpack --watch --mode development",
    "browser": "web-ext run -s dist -p usi",
    "prod": "webpack --watch --mode production --devtool none",
    "self-update-packages": "ncu -u --removeRange && npm install",
    "lint-fix": "tslint --fix -p .",
    "build": "webpack --mode production --devtool none && web-ext build -s dist"
  },
  "author": "usi <>",
  "license": "LGPL",
  "dependencies": {
    "material-design-icons-iconfont": "5.0.1",
    "vue": "2.6.11",
    "vuetify": "2.2.6",
    "vuex": "3.1.2"
  },
  "devDependencies": {
    "@mdi/font": "4.8.95",
    "@mdi/js": "4.8.95",
    "@types/jquery": "3.3.31",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.4.2",
    "file-loader": "5.0.2",
    "npm-check-updates": "4.0.1",
    "replace-in-file-webpack-plugin": "1.0.6",
    "tslint": "6.0.0",
    "ts-loader": "6.2.1",
    "typescript": "3.7.5",
    "vue-loader": "15.8.3",
    "vue-template-compiler": "2.6.11",
    "web-ext": "4.0.0",
    "web-ext-types": "3.2.1",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10"
  }
}
