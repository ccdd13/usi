{
  "name": "usi",
  "version": "0.5.13",
  "description": "usi, userscript injector for Mozilla Firefox",
  "scripts": {
    "dev": "webpack --watch --mode development",
    "browser": "web-ext run -s dist -p usi",
    "browser-fresh": "web-ext run -s dist",
    "prod": "webpack --watch --mode production --devtool none",
    "self-update-packages": "ncu -u --removeRange && npm install",
    "lint-fix": "tslint --fix -p .",
    "build": "webpack --mode production --devtool none && web-ext build -s dist"
  },
  "author": "usi <>",
  "license": "LGPL",
  "dependencies": {
    "material-design-icons-iconfont": "6.1.0",
    "vue": "2.6.12",
    "vuetify": "2.3.10",
    "vuex": "3.5.1"
  },
  "devDependencies": {
    "@mdi/font": "5.6.55",
    "@mdi/js": "5.6.55",
    "@types/jquery": "3.5.1",
    "copy-webpack-plugin": "6.1.1",
    "css-loader": "4.3.0",
    "file-loader": "6.1.0",
    "npm-check-updates": "9.0.2",
    "replace-in-file-webpack-plugin": "1.0.6",
    "style-loader": "1.2.1",
    "ts-loader": "8.0.4",
    "tslint": "6.1.3",
    "typescript": "4.0.3",
    "vue-loader": "15.9.3",
    "vue-template-compiler": "2.6.12",
    "web-ext": "5.1.0",
    "web-ext-types": "3.2.1",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12"
  }
}
